{
  "uid": "quality-overview",
  "title": "Quality Overview",
  "tags": [
    "automated-testing",
    "quality"
  ],
  "timezone": "",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Validation Confidence Distribution",
      "type": "heatmap",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "sum(rate(validation_confidence_score_bucket[5m])) by (le)",
          "format": "heatmap",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "show": true
        },
        "tooltip": {
          "show": true
        }
      }
    },
    {
      "id": 2,
      "title": "Human Validation Rate",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "sum(rate(test_executions_total{validation=\"human\"}[5m])) / sum(rate(test_executions_total[5m])) * 100",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      }
    },
    {
      "id": 3,
      "title": "Accuracy Metrics",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "sum(rate(test_accuracy_total{outcome=\"pass\"}[5m])) / sum(rate(test_accuracy_total[5m])) * 100",
          "legendFormat": "Pass Rate",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      }
    }
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h"
    ]
  },
  "annotations": {
    "list": []
  }
}
